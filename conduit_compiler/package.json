{
  "name": "conduit",
  "version": "0.0.1",
  "description": "Universal typing",
  "scripts": {
    "test:unit": "jest unit/",
    "compile": "tsc -b src/main/",
    "pkg:mac": "npm run compile && npx pkg dist/server.js --target macos"
  },
  "author": "Jeremy",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.5",
    "jest": "^25.5.4",
    "pkg": "^4.4.8",
    "prettier": "^2.0.5",
    "ts-jest": "^25.5.1",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "yauzl": "^2.10.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/test/**/*.[jt]s",
      "**/?(*.)+(spec|test).[tj]s"
    ]
  }
}
