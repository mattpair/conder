  "devDependencies": {
    "@types/jest": "^26.0.8",
    "@types/mongodb": "^3.5.27",
    "@types/node": "^14.10.1",
    "etcd3": "^1.1.0",
    "jest": "^26.2.2",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.4",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "mongodb": "^3.6.2",
    "noble-ed25519": "https://github.com/paulmillr/noble-ed25519/archive/1.0.2.tar.gz"
  },
  "jest": {
    "preset": "ts-jest",
    "testMatch": [
      "**/?(*.)+(spec).[tj]s"
