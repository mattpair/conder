    "tspeg": "^2.1.0",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "conder_core": "file:../conder_core/"
  },
  "jest": {
    "preset": "ts-jest",
    "testMatch": [
      "**/test/**/*.[jt]s",
      "**/?(*.)+(spec|test).[tj]s"
